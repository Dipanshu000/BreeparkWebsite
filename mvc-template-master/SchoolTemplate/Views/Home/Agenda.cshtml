@using SchoolTemplate.Database;
@model List<Festival>
@{
    ViewData["Title"] = "Agenda";
}

<h1>Agenda</h1>


<ul>
    @if (Model != null)
    {
        foreach (Festival festival in Model)
        {
            <li>
                <a href="/festival/@festival.Id">@festival.Naam @festival.Beschrijving @festival.Datum.ToString("D") @festival.Tijd @festival.Prijs </a>
            </li>
        }
    }
</ul>

